<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">

  <configuration>
   <settings>
       <!-- 자동으로 카멜케이스 규칙으로 변환 -->
       <setting name="mapUnderscoreToCamelCase" value="true"/>
       <!--파라미터에 null값이 있을 경우 에러 처리 -->
       <setting name="jdbcTypeForNull" value="VARCHAR" />
   </settings>

	<typeAliases>
		<typeAlias type="java.util.Map" alias="map" />

		<typeAlias type="kr.or.annapharm.dto.ProductVO" alias="product" />
		<typeAlias type="kr.or.annapharm.dto.BoardVO" alias="board" />
		<typeAlias type="kr.or.annapharm.dto.PatientVO" alias="patient" />
		<typeAlias type="kr.or.annapharm.dto.PrescriptionVO" alias="prescription" />
		<typeAlias type="kr.or.annapharm.dto.ClassficationVO" alias="classfication" />
		<typeAlias type="kr.or.annapharm.dto.MenuVO" alias="menu" />
		<typeAlias type="kr.or.annapharm.dto.AnnapharmInVO" alias="annapharmIn" />
		<typeAlias type="kr.or.annapharm.dto.MemberVO" alias="member" />
		<typeAlias type="kr.or.annapharm.dto.ReplyVO" alias="reply" />
		<typeAlias type="kr.or.annapharm.dto.PrescriptionHistoryVO" alias="prescriptionHistory" />
		<typeAlias type="kr.or.annapharm.dto.PrescriptionDetailVO" alias="prescriptionDetail" />
	</typeAliases>

</configuration>