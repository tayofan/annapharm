package kr.or.annapharm.dto;

public class ChemistVO extends MemberVO {

	private String chemistNum;
	private String pharmacyCode;
	private String pharmacyName;

	private String authority = "일반";


	public void setChemistVO(MemberVO member) {
		this.setMemId(member.getMemId());
		this.setMemPwd(member.getMemPwd());
		this.setMemName(member.getMemName());
		this.setMemTel(member.getMemTel());
		this.setMemAddr(member.getMemAddr());
		this.setMemAddr2(member.getMemAddr2());
		this.setMemZipcode(member.getMemZipcode());
		this.setMemAuthority(member.getMemAuthority());
		this.setMemEmail(member.getMemEmail());
		this.setMemRegdate(member.getMemRegdate());
		this.setMemRegnum(member.getMemRegnum());

	}



	public String getChemistNum() {
		return chemistNum;
	}

	public void setChemistNum(String chemistNum) {
		this.chemistNum = chemistNum;
	}

	public String getPharmacyCode() {
		return pharmacyCode;
	}

	public void setPharmacyCode(String pharmacyCode) {
		this.pharmacyCode = pharmacyCode;
	}

	public String getPharmacyName() {
		return pharmacyName;
	}

	public void setPharmacyName(String pharmacyName) {
		this.pharmacyName = pharmacyName;
	}

	public String getAuthority() {
		return authority;
	}

	public void setAuthority(String authority) {
		this.authority = authority;
	}

	@Override
	public String toString() {
		return "ChemistVO [chemistNum=" + chemistNum + ", pharmacyCode=" + pharmacyCode + ", pharmacyName="
				+ pharmacyName + ", authority=" + authority + "]";
	}



}
