<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>

	<div class="content-wrapper">

		<section class="content">
			<div class="container-fluid">
			<div class="row">
					<div class="col-lg-12">
						<div class="card card-outline"></div>
					</div>
				</div>
			
				<!-- Main row -->
				<div class="row">
					<!-- Left col -->
					<section class="col-lg-6 connectedSortable ui-sortable">
					
					<div class="row">
						<div class="col-lg-6 col-6">
						
						<!-- small box -->
						<div class="small-box bg-info" style="height: 450px;">
							<div class="inner">
								<h3>150</h3>

								<p>New Orders</p>
							</div>
						</div>
					</div>
					<div class="col-lg-6 col-6">
						<!-- small box -->
						<div class="small-box bg-info" style="height: 450px;">
							<div class="inner">
								<h3>150</h3>

								<p>New Orders</p>
							</div>
						</div>
					</div>
					</div>
					
						<!-- Custom tabs (Charts with tabs)-->
						<div class="card">
							<div class="card-header ui-sortable-handle" style="cursor: move;">
								<h3 class="card-title">
									<i class="fas fa-chart-pie mr-1"></i> Sales
								</h3>
							</div>
							<!-- /.card-header -->
							<div class="card-body" style="    height: 350px;">
								
							</div>
							<!-- /.card-body -->
						</div>
						<!-- /.card -->
					</section>
					<!-- /.Left col -->
					
					
					<!-- right col (We are only adding the ID to make the widgets sortable)-->
					<section class="col-lg-6 connectedSortable ui-sortable">

						<!-- Map card -->
						<div class="card bg-gradient-primary">
							<div class="card-body">
							
							</div>
							<!-- /.card-body-->
						</div>
						<!-- /.card -->

						<!-- solid sales graph -->
						<div class="card bg-gradient-info">
							<div class="card-body">
							
							</div>
							<!-- /.card-body -->
						</div>
						<!-- /.card -->
					</section>
					<!-- right col -->
				</div>
				<!-- /.row (main row) -->
			</div>
			<!-- /.container-fluid -->
		</section>
	</div>
</body>
</html>