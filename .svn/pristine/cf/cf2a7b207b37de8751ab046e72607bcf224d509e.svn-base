<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Pharmacy-Mapper">
	<select id="selectPharmacyName" resultType="pharmacy">
		SELECT
   			 pharmacy_code
   			,pharmacy_name
  			,pharmacy_addr
		FROM
    		pharmacy
	</select>
</mapper>



